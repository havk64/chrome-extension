{
    "manifest_version": 2,
    "name": "All Images in all websites",
    "version": "0.1",
    "browser_action": {
        "default_icon": {                    // optional
            "19": "img/icon38.png"           // optional
            //"38": "images/icon38.png"            // optional
        },
        "default_title": "All Images"      // optional; shown in tooltip
        //"default_popup": "popup.html"        // optional
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ]
}
